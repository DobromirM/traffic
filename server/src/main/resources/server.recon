@server {
  @plane("traffic") {
    class: "swim.traffic.TrafficPlane"
  }
  @store {
    path: "/tmp/swim-traffic/"
  }
  @http(port: 9001) {
    plane: "traffic"
    documentRoot: "../ui/"
    @websocket {
      serverCompressionLevel: 0# -1 = default; 0 = off; 1-9 = deflate level
      clientCompressionLevel: 0# -1 = default; 0 = off; 1-9 = deflate level
    }
  }
}
